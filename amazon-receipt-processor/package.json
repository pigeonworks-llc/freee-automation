{
  "name": "amazon-receipt-processor",
  "version": "1.0.0",
  "description": "Amazon receipt processor for freee accounting system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "process": "node dist/cli.js",
    "download": "node dist/download.js",
    "auto-download": "node dist/auto-download.js",
    "sync": "npm run download && npm run process",
    "dev": "ts-node src/cli.ts",
    "dev:download": "ts-node src/download.ts",
    "dev:auto-download": "ts-node src/auto-download.ts"
  },
  "keywords": [
    "amazon",
    "receipt",
    "freee",
    "beancount"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@accounting-system/shared": "*",
    "pdf-parse": "^1.1.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.1",
    "glob": "^10.3.10",
    "form-data": "^4.0.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/pdf-parse": "^1.1.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  }
}
